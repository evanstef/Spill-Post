{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "^/build/(.*)$",
            "dest": "/public/build/$1",
            "continue": true
        },
        {
            "src": "^/js/(.*)$",
            "dest": "/public/js/$1",
            "continue": true
        },
        {
            "src": "^/css/(.*)$",
            "dest": "/public/css/$1",
            "continue": true
        },
        {
            "src": "^/favicon\\.ico$",
            "dest": "/public/favicon.ico",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://spill-post-project.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "ASSET_URL": "https://spill-post-project.vercel.app",
        "DB_CONNECTION": "pgsql"
    }
}
